    "version": "0.2.0",
    "configurations": [
        {
            "name": "pastar",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/build_debug/pastar", 
            "args": ["--seed=0", "--map=benchmark/Paris_1_256.map", "--agents=benchmark/Paris_1_256.map.scen", 
                    "--output=test.csv", "--outputPaths=test_path.txt", "--algo=HDA*", "--trialNum=1",
                    ],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            "environment": [],
            "externalConsole": false,
            "MIMode": "lldb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ],
            "preLaunchTask": "Cmake Debug",
            //"miDebuggerPath": "/usr/bin/gdb"
        },
        {
            "name": "mpi",
            "type": "cppdbg",
            "request": "attach",
            "processId": "${command:pickProcess}",
            "program": "${workspaceFolder}/build_debug/pastar", 
            "MIMode": "lldb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ],
            "preLaunchTask": "Cmake Debug",
            //"miDebuggerPath": "/usr/bin/gdb"
        }

    ]
}